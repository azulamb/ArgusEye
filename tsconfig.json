{
	"compilerOptions": {
		"outDir": "./dest",
		"module": "commonjs",
		"target": "es2017",
		"lib": ["es2017"],
		"moduleResolution": "node",
		"strictNullChecks": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": false,
		"sourceMap": false
	},
	"include": [
		"./src/**/*.ts"
	]
}
